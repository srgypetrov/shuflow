{
	"importOrder": [".app/(.*)$", ".lib/(.*)$", "^[./]"],
	"importOrderSeparation": true,
	"importOrderSortSpecifiers": true,
	"overrides": [
		{
			"files": "*.svelte",
			"options": {
				"parser": "svelte"
			}
		}
	],
	"plugins": [
		"prettier-plugin-svelte",
		"prettier-plugin-organize-imports",
		"prettier-plugin-tailwindcss"
	],
	"printWidth": 100,
	"singleQuote": true,
	"semi": false,
	"trailingComma": "none",
	"useTabs": true
}
